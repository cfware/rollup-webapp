{
	"name": "@cfware/rollup-webapp",
	"version": "0.0.0",
	"description": "Web Application Template",
	"private": true,
	"scripts": {
		"test": "xo && nyc ava -sv",
		"start": "gulp start",
		"prepack": "gulp rebuild"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/rollup-webapp.git"
	},
	"keywords": [
		"rollup",
		"babel",
		"customelements",
		"browser"
	],
	"files": [
		"wwwroot/**"
	],
	"author": "Corey Farrell",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/cfware/rollup-webapp/issues"
	},
	"homepage": "https://github.com/cfware/rollup-webapp#readme",
	"devDependencies": {
		"@babel/core": "^7.3.4",
		"@babel/preset-env": "^7.3.4",
		"@babel/register": "^7.0.0",
		"@cfware/ava-selenium-manager": "^0.2.0",
		"@cfware/eslint-config-browser": "^0.1.2",
		"@cfware/fastify-test-helper": "^0.2.0",
		"@cfware/shadow-element": "^0.4.0",
		"@gulp-sourcemaps/map-sources": "^1.0.0",
		"ansi-colors": "^3.2.3",
		"ava": "^1.2.1",
		"babel-plugin-bare-import-rewrite": "^1.3.1",
		"babel-plugin-bundled-import-meta": "^0.2.2",
		"babel-plugin-istanbul": "^5.1.1",
		"babel-plugin-remove-ungap": "^1.0.0",
		"babel-plugin-template-html-minifier": "^2.2.1",
		"babel-plugin-transform-commonjs": "^1.1.5",
		"babel-preset-minify": "^0.5.0",
		"camelcase": "^5.0.0",
		"chromedriver": "^2.46.0",
		"del": "^3.0.0",
		"esm": "^3.2.7",
		"fastify": "^2.0.0",
		"fastify-babel": "^1.1.0",
		"fastify-static": "^2.3.4",
		"geckodriver": "^1.16.0",
		"gulp": "^4.0.0",
		"gulp-if": "^2.0.2",
		"gulp-markdown": "^3.0.0",
		"gulp-rename": "^1.4.0",
		"gulp-replace": "^1.0.0",
		"gulplog": "^1.0.0",
		"merge2": "^1.2.3",
		"nyc": "^13.3.0",
		"pump": "^3.0.0",
		"rollup": "^1.4.0",
		"rollup-plugin-babel": "^4.3.2",
		"vinyl-rollup": "^0.4.2",
		"xo": "^0.24.0"
	},
	"nyc": {
		"all": true,
		"es-modules": true,
		"exclude": [
			"**/node_modules/**",
			"babel.config.js",
			"coverage/**",
			"wwwroot/**",
			"gulpfile.*",
			"helpers/**",
			"test/**"
		],
		"reporter": [
			"text",
			"html"
		]
	},
	"xo": {
		"overrides": [
			{
				"files": [
					"html/**",
					"test/fixtures/**"
				],
				"extends": "@cfware/eslint-config-browser"
			}
		]
	},
	"ava": {
		"require": [
			"@babel/register"
		]
	}
}
